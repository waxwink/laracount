{
    "name": "waxwink/laracount",
    "description": "Accounting service for laravel",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "waxwink",
        "laravel",
        "accounting",
        "payment",
        "wallet",
        "deposit",
        "withdraw",
        "transaction",
        "laracount"
    ],

    "authors": [
        {
            "name": "Mohamadreza Pishdad",
            "email": "mpishdad89@google.com",
            "role": "Developer"
        }
    ],

    "require": {
        "php": "^8.0",
        "waxwink/accounting": "0.1",
        "illuminate/database": "^8.67",
        "illuminate/container": "^8.67",
        "illuminate/contracts": "^8.67"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.22"
    },
    "autoload": {
        "psr-4": {
            "Waxwink\\Laracount\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waxwink\\Laracount\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waxwink\\Laracount\\LaracountServiceProvider"
            ]
        }
    }
}
